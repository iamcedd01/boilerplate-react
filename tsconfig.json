{
  "extends": "@tsconfig/create-react-app/tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "baseUrl": "./src",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "incremental": true,
    "paths": {
      "components/*": ["./components/*"],
      "config/*": ["./config/*"],
      "pages/*": ["./pages/*"],
      "utils/*": ["./utils/*"]
    }
  },
  "include": ["src"]
}
